@{
    Layout = "../Shared/_LoginLayout.cshtml";
}

<img class="bigLogo" src="data:image/jpeg;base64," alt="Alternate Text" />

<form class="signInForm" action="/" method="post">
    <label>Brugernavn</label>
    <input type="text" name="name" value="" />
    <label>Adgangskode</label>
    <input type="password" name="password" value="" />

    <input class="bigButton loginButton" type="submit" name="submit" value="Log ind" />
</form>

<a class="smallButton newUserButton" href="#">Ny bruger</a>
<button class="smallButton helpButton">Hjælp</button>