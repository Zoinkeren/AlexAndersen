@{
    Layout = "../Shared/_LoginLayout.cshtml";
}

<img class="bigLogo" src="data:image/jpeg;base64," alt="Alternate Text" />

<form class="signInForm" action="/" method="post">
    <label>Brugernavn</label>
    <input type="text" name="name" value="" />
    <label>Adgangskode</label>
    <input type="password" name="password" value="" />

    <input class="bigButton loginButton" type="submit" name="submit" value="Log ind" />
</form>

<a class="smallButton newUserButton" href="/login/SignUp">Ny bruger</a>
<button class="smallButton helpButton">Hjælp</button>

<div class="popUp helpPopUp">
    <div class="popUpBox">
        <h4>Få hjælp til login</h4>
        <p>Ring til telefon nr.:</p>
        <p class="helpNo">+45 12 13 14 15</p>
        <button class="popUpClose">OK</button>
    </div>
</div>

<script>
    document.querySelector(".helpButton").addEventListener("click", function () {
        document.querySelector(".popUp").style.display = "block";
    });

    document.querySelector(".popUpClose").addEventListener("click", function () {
        document.querySelector(".popUp").style.display = "none";
    });



</script>